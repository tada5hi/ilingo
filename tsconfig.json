{
    "extends": "@tada5hi/tsconfig",
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "declarationDir": "dist",
        "lib": [
            "ESNext"
        ],
        "module": "ESNext",
        "moduleResolution": "node",
        "target": "ESNext",
    },
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "node_modules/**/*",
        "dist"
    ]
}
